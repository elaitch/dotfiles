! xft fonts

Xft.antialias:  1
Xft.dpi:        96
Xft.hinting:    1
Xft.hintstyle:  hintslight
Xft.rgba:       rgb

! urxvt fonts

URxvt.font:-*-termsyn-medium-*-*-*-14-*-*-*-*-*-*-*
!URxvt.font:xft:source code pro medium:pixelsize=11:antialias=true:hinting=true
URxvt.italicFont:xft:droid sans mono:pixelsize=11:italic:antialias=true:hinting=true
URxvt.xftAntialias: true


! urxvt options

URxvt.boldMode: false
URxvt.borderless: true
URxvt.internalBorder: 3
URxvt.cursorBlink: false
URxvt.cursorUnderline: false
URxvt.depth:       16
URxvt.imLocale: en_US.utf8
URxvt.jumpScroll: true
URxvt.letterSpace: 0
URxvt.loginShell: true
URxvt.saveLines: 8192
URxvt.scrollBar: false
URxvt.skipBuiltinGlyphs: true
URxvt.transparent: false
URxvt.underlineURLs: true
urxvt.geometry: 80x40
urxvt.urgentOnBell: true


! perl extensions

URxvt.perl-ext-common : default,clipboard,url-select,keyboard-select
URxvt.keysym.M-u:       perl:url-select:select_next
URxvt.keysym.M-Escape:  perl:keyboard-select:activate
URxvt.keysym.M-s:       perl:keyboard-select:search
URxvt.url-select.launcher: /usr/bin/firefox
URxvt.url-select.underline: true


!colors

URxvt*background:            #222222
!URxvt*background: 		#21242F
URxvt*foreground:            #e6e6e6
URxvt*cursorColor:           #EFEFEF
URxvt*colorBD:               #FFFFFF
URxvt*colorUL:               #DFFF00

!black
*color0:#222222
*color8:#777777

!red
*color1:#FF3D84
*color9:#FF3D84

!green
*color2:#3DFF57
*color10:#7AFF8C

!yellow
*color3:#DFFF00
*color11:#EDFF7A

!blue
*color4:#005EFF
*color12:#5794FF

!magenta
*color5:#BC47FF
*color13:#C561FF

!cyan
*color6:#00DDFF
*color14:#0AFFFF


!white
*color7:#fdfdfd
*color15:#efefef
